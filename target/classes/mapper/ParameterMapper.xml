<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ycj.student.mapper.ParameterMapper">


    <select id="list" resultType="com.ycj.student.dto.parameter.ParameterDTO">
        SELECT t.* FROM edu_parameter t where t.delete_flag = 0 and t.parameter_type = #{parameterType}
    </select>

</mapper>
